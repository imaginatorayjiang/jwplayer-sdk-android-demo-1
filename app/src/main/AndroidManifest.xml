<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.jwplayer.opensourcedemo">

    <application
        android:name="com.jwplayer.opensourcedemo.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <!-- TODO Set your license key -->
        <meta-data
            android:name="JW_LICENSE_KEY"
            android:value="your license key "/>

        <activity
            android:name=".JWPlayerViewExample"
            android:configChanges="keyboard|keyboardHidden|orientation|screenSize"
            android:label="@string/app_name">
        </activity>
        <activity
            android:name=".JWPlayerFragmentExample"
            android:configChanges="keyboard|keyboardHidden|orientation|screenSize">
        </activity>

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name=".videolist_v1.VideoListActivity"
            android:configChanges="keyboard|keyboardHidden|orientation|screenSize"/>
    </application>

</manifest>
